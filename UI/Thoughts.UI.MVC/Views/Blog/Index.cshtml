@model BlogIndexWebModel
@{
    ViewBag.Title = "Лента записей";
}

<!-- Blogs - список заметок блога, страница блогов -->
<header class="major">
    <h1>@ViewBag.Title</h1>
</header>

<div class="posts">
    @foreach (var e in Model.Posts)
    {
        <vc:post-cart post="e"></vc:post-cart>
    }
</div>